<h1>Add a new Kitten</h1>
<%= form_for @kitten, url: kittens_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :cuteness %>
  <%= f.text_field :cuteness %>

  <%= f.label :softness %>
  <%= f.text_field :softness %>

  <%= f.submit %>

<% end %>
